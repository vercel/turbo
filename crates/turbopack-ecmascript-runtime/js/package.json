{
  "name": "@vercel/turbopack-ecmascript-runtime",
  "version": "0.0.0",
  "description": "Turbopack EcmaScript runtime code and utilities",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "check": "run-p check:*",
    "check:nodejs": "tsc -p src/nodejs",
    "check:dev-client": "tsc -p src/dev/client",
    "check:dev-runtime-base": "tsc -p src/dev/runtime/base",
    "check:dev-runtime-dom": "tsc -p src/dev/runtime/dom",
    "check:dev-runtime-edge": "tsc -p src/dev/runtime/edge"
  },
  "exports": {
    ".": "./src/main.js",
    "./*": "./src/*.ts"
  },
  "dependencies": {
    "@types/node": "^18.11.11"
  },
  "devDependencies": {
    "@next/react-refresh-utils": "^14.1.0",
    "npm-run-all": "^4.1.5"
  }
}
