{
  "baseBranch": "origin/main",
  "env": [
    "CONVERTKIT_API_KEY",
    "CONVERTKIT_API_SECRET",
    "CONVERTKIT_FORM_ID"
  ],
  "pipeline": {
    "test": {
      "outputs": [
        "coverage/**/*"
      ],
      "dependsOn": [
        "^build"
      ]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "build": {
      "outputs": [
        "dist/**/*",
        ".next/**/*"
      ],
      "dependsOn": [
        "^build"
      ]
    },
    "cli#build": {
      "outputs": [
        "turbo",
        "turbo.exe"
      ],
      "dependsOn": []
    },
    "create-turbo#test": {
      "dependsOn": [
        "create-turbo#build"
      ],
      "outputs": []
    }
  }
}
